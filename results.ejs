<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>PromoterLocate</title>
    <link
      rel="stylesheet"
      href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css"
      integrity="sha384-JcKb8q3iqJ61gNV9KGb8thSsNjpSL0n8PARn9HuZOnIxN0hoP+VmmDGMN5t9UJ0Z"
      crossorigin="anonymous"
    />
  </head>
  <body>
    <%- include('navbar') -%>
    <div class="container col px-md-5 p-5">
      <table class="table table-striped table-bordered">
        <thead>
          <tr>
            <th scope="col">Rank</th>
            <th scope="col">-35 Position</th>
            <th scope="col">-10 Position</th>
            <th scope="col">-35 Hexamer</th>
            <th scope="col">-10 Hexamer</th>
            <th scope="col">Predicted Strength</th>
          </tr>
        </thead>
        <tbody>
          <% for(var i=0; i < data.length; i++) { %>
          <tr>
            <td><%= i+1 %></td>
            <td><%= data[i].position35 %></td>
            <td><%= data[i].position10 %></td>
            <td><%= data[i].hexamer35 %></td>
            <td><%= data[i].hexamer10 %></td>
            <td><%= data[i].prdStr %></td>
          </tr>
          <% } %>
        </tbody>
      </table>
    </div>
  </body>
</html>
